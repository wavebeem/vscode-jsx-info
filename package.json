{
  "name": "vscode-jsx-info",
  "displayName": "JSX Info",
  "description": "VSCode integration for jsx-info",
  "version": "0.1.3",
  "publisher": "wavebeem",
  "main": "./dist/extension.bundle.js",
  "icon": "img/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/wavebeem/vscode-jsx-info.git"
  },
  "bugs": {
    "url": "https://github.com/wavebeem/vscode-jsx-info/issues"
  },
  "homepage": "https://github.com/wavebeem/vscode-jsx-info#readme",
  "scripts": {
    "upgrade": "npm-check -u",
    "vscode:prepublish": "npm run build",
    "build": "webpack --mode production",
    "start": "webpack --mode development --watch",
    "postversion": "git push && git push --tags && vsce publish"
  },
  "devDependencies": {
    "@types/node": "^14.14.16",
    "@types/vscode": "^1.45.0",
    "npm-check": "^5.9.2",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.12",
    "typescript": "^4.0.2",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.0"
  },
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:jsxInfo"
  ],
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "jsxInfo",
          "name": "JSX Info"
        }
      ]
    },
    "commands": [
      {
        "command": "jsxInfo.run",
        "title": "JSX Info: Run"
      },
      {
        "command": "jsxInfo.refresh",
        "title": "JSX Info: Refresh"
      }
    ]
  },
  "dependencies": {
    "jsx-info": "^3.0.0"
  }
}
