{
  "name": "vscode-jsx-info",
  "displayName": "jsx-info",
  "description": "VSCode integration for jsx-info",
  "version": "0.0.1",
  "publisher": "Brian Mock <brian@wavebeem.com>",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc",
    "start": "tsc --watch"
  },
  "devDependencies": {
    "@types/node": "^12.12.0",
    "@types/vscode": "^1.45.0",
    "prettier": "^2.2.1",
    "typescript": "^4.0.2"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:jsxInfo"
  ],
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "jsxInfo",
          "name": "JSX Info"
        }
      ]
    },
    "commands": [
      {
        "command": "jsxInfo.run",
        "title": "JSX Info: Run"
      },
      {
        "command": "jsxInfo.refresh",
        "title": "JSX Info: Refresh"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "jsxInfo.run"
        }
      ],
      "view/title": [
        {
          "command": "jsxInfo.run",
          "when": "view == jsxInfo"
        },
        {
          "command": "jsxInfo.refresh",
          "when": "view == jsxInfo"
        }
      ]
    }
  },
  "dependencies": {
    "jsx-info": "^3.0.0"
  }
}
